generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cscCompiledExtendedStats {
  id                 Int                 @id @default(autoincrement())
  createdAt          DateTime            @default(now())
  season             Int                 @db.Integer
  throughMatchDay    Int                 @db.Integer
  extendedStatsJson      Json                @db.JsonB
}

model cscMatchAnalysis {
  id                    Int                 @id @default(autoincrement())
  createdAt             DateTime            @default(now())
  season                Int                 @db.Integer
  matchDay              Int                 @db.Integer
  matchId               Int                 @db.Integer
  tier                  String              @db.VarChar(255)
  matchAnalysisJson     Json                @db.JsonB
}

